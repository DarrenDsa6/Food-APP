        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Food_Ordering_App"
        tools:targetApi="31">
        <activity android:name=".Activity.nonveg"/>
        <activity
            android:name=".Activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.veg"
            android:exported="false"/>
        <activity android:name=".Activity.allpizzas"/>
        <activity android:name=".Activity.pizzas.margarita"/>
        <activity android:name=".Activity.pizzas.farmhouse"/>
        <activity android:name=".Activity.pizzas.pepperoni"/>
        <activity android:name=".Activity.pizzas.peppypaneer"/>
        <activity android:name=".Activity.pizzas.barbeque"/>
        <activity android:name=".Activity.pizzas.mexican"/>
        <activity android:name=".Activity.pizzas.fiesta"/>
        <activity android:name=".Activity.pizzas.supreme"/>

    </application>

</manifest>